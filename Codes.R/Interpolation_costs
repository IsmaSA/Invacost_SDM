for(i in tm_list){
	pred.l[[paste0(i,"t")]]=c(pred.l[[i]],"time")
}


full_gamma <-c(0,0,0,0,0,NA,10) #named "full" gamma vector to make the optimization function (scale_fit_fun) faster
names(full_gamma)=c(pos.pred, "sd","b0")
full_gamma2=full_gamma
full_gamma2[] <-NA


